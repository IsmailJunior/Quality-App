            "./app/_assets/images/web/quality_website_services.jpeg",
            "./app/_assets/images/marketing/stats_1.jpeg",
            "./app/_assets/images/marketing/stats_2.jpeg",
            "./app/_assets/images/marketing/stats_3.jpeg",
            "./app/_assets/images/marketing/stats_4.jpeg",
            "./app/_assets/images/app_cat.png",
            "./app/_assets/images/design_cat.png",
            "./app/_assets/images/copy_cat.png",
            "./app/_assets/images/marketing_cat.png",
            "./app/_assets/images/motion_cat.png",
            "./app/_assets/images/printing_cat.png",
            "./app/_assets/images/reel_cat.png",
            "./app/_assets/images/tvc_cat.png",
            "./app/_assets/images/web_cat.png",
            "./app/_assets/images/onboarding2.png",
            "./app/_assets/images/onboarding3.png",
            "./app/_assets/videos/composition.mp4"
