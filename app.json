{
  "expo": {
    "name": "quality-app",
    "slug": "quality-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/_assets/images/adaptive-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#000000",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/_assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.qualityapp"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./app/_assets/images/adaptive-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-video",
      [
        "expo-asset",
        {
          "assets": [
            "./app/_assets/images/graphic_design/auto_chicken.png",
            "./app/_assets/images/graphic_design/dar_ala3rajy.png",
            "./app/_assets/images/graphic_design/english_school.png",
            "./app/_assets/images/graphic_design/global_degree.png",
            "./app/_assets/images/graphic_design/high_class.png",
            "./app/_assets/images/graphic_design/muhanad_alsudani.png",
            "./app/_assets/images/graphic_design/tareeq_alsultan.png",
            "./app/_assets/images/copy_writing/planing.png",
            "./app/_assets/images/copy_writing/presenting.png",
            "./app/_assets/images/copy_writing/writing_plan.png",
            "./app/_assets/images/motion_graphic/iphone_3d.png",
            "./app/_assets/images/motion_graphic/joystick_3d.png",
            "./app/_assets/images/motion_graphic/monitor_3d.png",
            "./app/_assets/images/motion_graphic/stage_3d.png",
            "./app/_assets/images/motion_graphic/vr_3d.png",
            "./app/_assets/images/tvc/hacking.png",
            "./app/_assets/images/tvc/ipad.png",
            "./app/_assets/images/tvc/private_lecture.png",
            "./app/_assets/images/tvc/tests_lecture.png",
            "./app/_assets/images/tvc/youtube_lecture.png",
            "./app/_assets/images/web/4th_dim.jpeg",
            "./app/_assets/images/web/masakin_website_featuers.jpeg",
            "./app/_assets/images/web/masakin_website_hero.jpeg",
            "./app/_assets/images/web/masakin_website_list.jpeg",
